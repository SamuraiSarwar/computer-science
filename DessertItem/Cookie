     public class Cookie extends DessertItem{ 
     private double priceDozen; 
     private int numCookie; 
     //default constructor 
     //user defined constructor 
     public Cookie(String nm, int num, double ppDz) 
     { 
         super(nm); 
         numCookie = num; 
         priceDozen= ppDz; 
     }//user defined 
      public int getNumCookie()
      {
          return numCookie;
      }
      public double getPriceDozen()
      {
          return priceDozen;
      }
      public void setNumCookie(int num)
      {
          numCookie = num;
      }
      public void setPriceDozen(double price)
      {
          priceDozen = price;
      }
      public String toString()
      {
          return this.toString();
      }
     //accessor method - 2 ea 
     //mutator method - 2 ea 
     //getCost method - returns a double for the total price 
     //of cookies bought 
     //toString method - first call the super.toString+ with lables 
     //and numbercookies, price/dozen 
     @Override 
     public double getCost() 
     { 
         return 0; 
     }//getCost 
 }//Cookie 
