public class Icecream extends DessertItem{ 
     private double priceIce; 
     public Icecream()
    {
        priceIce = 0;
    }//default
     //default constructor 
     //user defined constructor 
     public Icecream(String nm, double cost) 
     { 
         super(nm); 
         priceIce= cost; 
     }//user defined
      public double getPricePound()
      {
          return priceIce;
      }
      public void setPricePound(double price)
      {
          priceIce = price;
      }
      public String toString()
      {
          return super.toString() + "\nSundae: $";
      }
      @Override
     public double getCost() 
     { 
         return priceIce;
     }    
}
     class Sundae extends Icecream{
          private double topping;
          
      }
