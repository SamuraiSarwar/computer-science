public class IceCream extends DessertItem {

    private double price;
    
    public IceCream() {

        price = 0;
    }//IceCream

    public IceCream( String iceCreamName, double newPrice) {
        super(iceCreamName, 'i');
        price = newPrice;
    }//IceCream

    @Override
    public double getCost() {
        return price;
    }//getCost
    public String getName(){
        return super.getName();
    }
    public void setPrice(double newNum) {

        price = newNum;
    }//setPrice
    
    public String toString() {
        return "\n" + super.getName() + " " + price;
    }//toString

}//IceCream
