import java.util.ArrayList;
public class vinDriver {
    public static void main(String args[])
    {
        String numString = "2";
        int num = Integer.valueof(numString);
        System.out.println(num);
        vinNum vin = new vimNum();
        int menu = 0;
        do{
        switch (menu)
        {
        case 1: String str = Util.getS("Enter VIN number");
                if(str.length()==17)
                {
                vin = new VinNum(str);
                System.out.println("Valid VIN:"+ vin.getValid());
                }
        else
            System.out.println("Not a valid length");
            break;
        case 2: break;
        }
    }while(menu == 6);
}
}
